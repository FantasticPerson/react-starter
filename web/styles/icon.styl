.user-list-icon-opt
  display inline-block
  height 27px
  float left
  width 67px
  background url(./icon/iconSprite.png) 0 0 no-repeat
  margin-right 5px

.user-list-add
  background-position -563px -594px

.user-list-modify
  background-position -563px -631px

.user-list-del
  background-position -563px -668px

.user-list-report
  background-position -563px -705px

.loading_bar
  border: solid 2px #86a5ad;
  background: #FFF url(./icon/progressBar_m.gif) no-repeat 10px 10px;

.user-list-head-bg
  background: url(./icon/headbg.jpg) 0 0 repeat-x;
  width: 100%;
  height: 94px;
  display: block;
  position: relative;

.user-list-header-logo {
  float: left;
  background: url(./icon/logo.jpg) no-repeat;
  width: 490px;
  height: 94px;
}
